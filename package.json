{
  "name": "react-actions-lab",
  "version": "0.1.0",
  "description": "Practice using actions to update React component state",
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "debug": "node-debug --hidden node_modules _mocha --watch",
    "debug-ide": "./bin/debug-ide",
    "copy-git-hooks": "./bin/copy-hooks",
    "test": "mocha -R mocha-multi --require test/root.js --reporter-options nyan=-,json=.results.json",
    "bundle": "browserify index.js -t babelify --exclude react/addons --exclude react/lib/ExecutionEnvironment --exclude react/lib/ReactContext -o bundle.js",
    "postinstall": "npm run copy-git-hooks"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/learn-co-curriculum/react-actions-lab.git"
  },
  "keywords": [
    "javascript",
    "flatiron",
    "learn"
  ],
  "author": "The Flatiron School",
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/learn-co-curriculum/react-actions-lab/issues"
  },
  "homepage": "https://github.com/learn-co-curriculum/react-actions-lab#readme",
  "devDependencies": {
    "jsdom": "^9.9.1",
    "mocha": "^3.2.0",
    "react-scripts": "^1.0.7",
    "react-test-renderer": "^15.5.4",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-airbnb": "^2.1.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "chai": "^3.5.0",
    "enzyme": "^2.7.0",
    "es6-promise": "^4.1.1",
    "expect": "^1.20.2",
    "isomorphic-fetch": "^2.2.1",
    "mocha-multi": "^0.11.0",
    "react": "^15.5.4",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.5.4",
    "sinon": "^1.17.7"
  }
}
